<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<!--설문 조사 버튼
기본 상태 / 클릭 상태
배경 나눠서 작성 -->
    <!-- 선택 시 배경-->
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid
                android:color="@color/gray5"/>
            <corners
                android:radius="5dp"/>
        </shape>
    </item>

    <!--디폴트 배경-->
    <item>
        <shape android:shape="rectangle">
            <corners
                android:radius="5dp"/>
            <stroke
                android:color="@color/gray5"
                android:width="1dp"/>
        </shape>
    </item>
</selector>